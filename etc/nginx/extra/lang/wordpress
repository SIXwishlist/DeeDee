index index.php;

location = /favicon.ico {
        log_not_found off;
        access_log off;
}

location = /robots.txt {
        allow all;
        log_not_found off;
        access_log off;
}

location / {
        try_files $uri $uri/ /index.php?q=$uri&$args;
}

location ~ .(eot|ttf|woff|otf)$ {
    add_header Access-Control-Allow-Origin *;
}

rewrite ^.*/files/(.*) /wp-includes/ms-files.php?file=$1;

